{
    "name": "cca-bheath/laravel-validations",
    "description": "A set of Laravel validation rules",
    "keywords": [
        "laravel",
        "validation",
        "rules"
    ],
    "type": "library",
    "require": {
        "php": "^7.2|^8.0"
    },
    "require-dev": {
        "brianium/paratest": "6.*",
        "friendsofphp/php-cs-fixer": "^2.18",
        "orchestra/testbench": "^6.12",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "CCA\\LaravelValidations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CCA\\LaravelValidations\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CCA\\LaravelValidations\\LaravelValidationsServiceProvider"
            ]
        }
    }
}
